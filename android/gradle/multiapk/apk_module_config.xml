<?xml version="1.0" encoding="utf-8"?>
<ApkModules>
    <!--
        0x00是共享资源
        0x01是Android系统资源，
        0x02是WebView资源(Android 5.0新增)
        0x7f这个一般不动

        0x10到0x7e的都可以用
    -->
    <Module packageName="com.multiapk.modules.computer" resourceId="0x20" />
    <Module packageName="com.multiapk.modules.mobile" resourceId="0x30" />
    <Module packageName="com.multiapk.modules.mobile.android" resourceId="0x40" />
    <Module packageName="com.multiapk.modules.mobile.ios" resourceId="0x50" />
    <Module packageName="com.multiapk.modules.mobile.order" resourceId="0x60" />
</ApkModules>