<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Default.Theme.App.Translucent.Normal" parent="Default.Theme.App.Normal">
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Default.Theme.App.Translucent.Animation" parent="Default.Theme.App.Animation">
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Default.Theme.App.Translucent.None" parent="Default.Theme.App.None">
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Default.Theme.App.Translucent.ActionBar" parent="Default.Theme.App.ActionBar">
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Default.Theme.App.Translucent.FullScreen" parent="android:Theme.Translucent.NoTitleBar.Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowBackground">@color/textColorPrimary</item>-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
</resources>